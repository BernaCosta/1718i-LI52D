<div class="row">
    <div class="panel col-xs-3">
        {{> favourites}}
    </div>
    <div class="col-xs-9">
        <ul>
            <li>Name: {{name}}</li>
            <li>Shortname: {{shortName}}</li>
            <li><img width='100px' src='{{crestUrl}}'/></li>
            <table>
                <tr>
                    <th>Name</th>
                    <th>Position</th>
                </tr>
                {{#each players}}
                    <tr>
                        <td>{{name}}</td>
                        <td>{{position}}</td>
                    </tr>
                {{/each}}
            </table>
        </ul>
    </div>
</div>