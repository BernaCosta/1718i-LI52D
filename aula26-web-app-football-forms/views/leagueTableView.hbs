<table class="table">
    <tr>
        <th>Position</th>
        <th>Name</th>
        <th>Points</th>
    </tr>
    {{#each standing}}
        <tr>
            <td>{{position}}</td>
            <td>
                <a href='/leagues/{{../leagueId}}/table/{{id}}'>
                    {{teamName}}
                </a>
            </td>
            <td>
                {{points}}
            </td>
        </tr>
    {{/each}}
</table>
